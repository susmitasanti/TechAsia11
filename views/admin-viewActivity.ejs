<!DOCTYPE html>
<html>

<head>
  <title>Dashboard</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


  <style>
    @import url('https://fonts.googleapis.com/css2?family=Inter:wght@200&display=swap');

    body {
      background-color: rgb(214, 225, 234);
      font-family: 'Inter', sans-serif;
    }

    .navbar {
      width: 100%;
      background-color: #22196b;
      overflow: hidden;
    }

    .navbar a {
      float: left;
      text-align: center;
      padding: 20px;
      color: white;
      text-decoration: none;
      font-size: 20px;
      margin-left: 0 px 12px;
    }

    .navbar img {
      margin-top: 2px;
      margin-left: 12px;
      float: left;
      height: 60px;
      width: 140px;
    }


    .dropdown2 {
      float: right;
      margin-right: 30px;
      font-size: 20px;
    }


    .dropdown2 .dropbtn2 {
      font-size: 20px;
      border: none;
      outline: none;
      color: white;
      padding: 20px;
      background-color: inherit;
      font-family: 'Inter', sans-serif;
    }


    .navbar a:hover,
    .navbar p:hover,
    .dropdown2:hover .dropbtn2 {
      background-color: red;
    }

    .dropdown-content2 {
      display: none;
      position: absolute;
      background-color: #f9f9f9;
      box-shadow: 0px 8px 12px 0px rgba(0, 0, 0, 0.2);
      z-index: 1;
      min-width: 146px;
      border: solid 1px;
    }

    .dropdown-content2 a {
      float: none;
      color: black;
      text-decoration: none;
      display: block;
      text-align: center;
      font-size: 20px;
      padding: 12px 12px;
    }


    .dropdown-content2 a:hover {
      background-color: #ddd;
      padding: 12px 12px;
    }

    .dropdown2:hover .dropdown-content2 {
      display: block;
    }

    .lower {
      background-color: rgb(232, 238, 240);
      border-radius: 10px;
      box-shadow: 0 14px 28px rgba(0, 0, 0, 0.25),
        0 10px 10px rgba(0, 0, 0, 0.22);
      position: relative;
      overflow: auto;
      margin-top: 20px;
      height: auto;
      padding: 10px;
    }

    #registration {
      font-family: 'Inter', sans-serif;
      border-collapse: collapse;
      overflow: hidden;
      width: 100%;
      font-size: 13px;
    }

    #registration td,
    #registration th {
      border: 1px solid #ddd;
      padding: 8px;
      overflow: hidden;
    }

    #registration tr:nth-child(even) {
      background-color: #f2f2f2;
    }

    #registration tr:hover {
      background-color: #ddd;
    }

    #registration th {
      padding-top: 12px;
      padding-bottom: 12px;
      text-align: left;
      background-color: #233789;
      color: white;
    }

    #delete {
      color: red;
      cursor: pointer;
    }

    .input-group,
    #search1 {
      padding: 10px;
      margin-top: 4px;
      margin-left: 230px;
      font-size: 15px;
      font-family: 'Inter', sans-serif;
      position: relative;

    }

    #button {
      padding: 9.5px;
      border: 2px solid #22196b;
      margin-top: 3.5px;
      font-size: 15px;
      background-color: #22196b;
      color: white;
      cursor: pointer;
    }

    #search1 {
      border: 1.5px solid #22196b;
    }

    #graph,
    #activity {
      border: 2px solid #22196b;
      background-color: #22196b;
      color: white;
      cursor: pointer;
      font-size: 13px;

    }

    #logout {
      font-size: 15px;
      background-color: #22196b;
      color: white;
      cursor: pointer;
      margin-left: -30px;
    }
  </style>


</head>

<body>
  <div class="navbar">
    <img src="techAsia-LOGO_horizontal1028.png" alt="">
    <a href="http://localhost:3000/admin-dashboard"><i class="fa fa-fw fa-search"></i> Users Data</a>
    <a href="http://localhost:3000/admin-activity"><i class="fa fa-fw fa-calculator"></i> Activity</a>
    <a href="http://localhost:3000/register"><i class="fa fa-fw fa-users"></i> Add User</a>
    <div class="dropdown2">
      <button class="dropbtn2" id="activity2"><i class="fa fa-fw fa-user"></i> Admin
        <i class="fa fa-caret-down"></i>
      </button>
      <div class="dropdown-content2">
        <a href="http://localhost:3000/admin-profile">Profile</a>
        <a href="http://localhost:3000/admin-registration">New Admin</a>
        <a href="#">
          <form id="Profile" action="/logout" method="GET" enctype="text/plain">
            &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<button id="logout">Logout</button>
          </form>
        </a>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="lower">
      <table class="table" id="registration">
        <thead>
          <tr>
            <th>Count</th>
            <th>Session Duration</th>
            <th>MAC ID</th>
            <th>Time</th>
            <th>Date</th>
          </tr>
        </thead>
        <tbody>
          <% users.forEach(function(user) { %>
            <tr>
              <td>
                <%= user.count %>
              </td>
              <td>
                <%= user.session_time %>
              </td>
              <td>
                <%= user.mac_id %>
              </td>
              <td>
                <%= user.time %>
              </td>
              <td>
                <%= String(user.date).slice(0, 16) %>
              </td>
            </tr>
            <% }); %>
        </tbody>
      </table>
    </div>
    <!-- <div class="lower">
     
    </div> -->
  </div>

  <script>
    document.getElementById("Profile").addEventListener("click", async function (event) {

      fetch(`http://localhost:3000/logout`, {
        method: "POST", // Replace with the appropriate HTTP method
        headers: {
          "Content-Type": "application/json",
        },
      })
        .then(response => response.json())
        .then(data => {
          // // Handle the API response data
          // console.log("Hulloo")
          console.log("Tata");
        })
        .catch(error => {
          // // Handle any errors that occurred during the API request
          console.log("Bubyee Error")

          console.error(error);
        });
    })
  </script>

</body>

</html>